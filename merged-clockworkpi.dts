	/* Fixed: Use i2c-gpio (software I2C) for PMIC on GPIO 0/1
	 * brcm,bcm2708-i2c is for BCM2708/BCM2835 hardware I2C which doesn't exist at this address on CM5
	 * The PMIC (AXP221) needs bit-bang I2C via GPIO 0/1 on RP1
	 */
	i2c0if {
		compatible = "i2c-gpio";
		status = "okay";
		/* GPIO 0 = SDA, GPIO 1 = SCL, both open-drain active-high */
		gpios = <0x2b 0x00 0x06>, <0x2b 0x01 0x06>;  /* rp1_gpio 0/1, GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN (0x06) */
		i2c-gpio,delay-us = <10>;  /* Increased to 10us (50kHz) for extreme reliability against noise */
		sda-open-drain;
		scl-open-drain;
		#size-cells = <0x00>;
		#address-cells = <0x01>;
		phandle = <0x106>;

		pmic@34 {
			phandle = <0x113>;
			/* IRQ disabled - causes probe failure with RP1 GPIO
			 * irq-gpios = <0x2b 0x02 0x00>;
			 * interrupts = <0x02 0x08>;
			 * interrupt-parent = <0x2b>;
			 * #interrupt-cells = <0x01>;
			 * interrupt-controller;
			 */
			reg = <0x34>;
			compatible = "x-powers,axp221", "simple-mfd";
			system-power-controller;

			adc {
				compatible = "x-powers,axp221-adc";
				#io-channel-cells = <0x01>;
			};

			ac_power_supply {
				phandle = <0x119>;
				compatible = "x-powers,axp221-ac-power-supply";
			};

			battery-power-supply {
				phandle = <0x118>;
