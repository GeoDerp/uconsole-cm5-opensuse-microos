	#address-cells = <0x02>;
	#size-cells = <0x02>;
	interrupt-parent = <0x01>;
	model = "Raspberry Pi Compute Module 5 Rev 1.0";

	backlight@0 {
		phandle = <0x10e>;
		default-brightness = <0x05>;
		backlight-control-gpios = <0x2b 0x09 0x00>;
		compatible = "ocp8178-backlight";
	};

	spdif_out: spdif-transmitter {
		compatible = "linux,spdif-dit";
		#sound-dai-cells = <0x00>;
		status = "okay";
		phandle = <0x201>;
	};

	uconsole_audio: uconsole-audio-card {
		compatible = "simple-audio-card";
		phandle = <0x202>;
		simple-audio-card,name = "uConsole Audio";
		status = "okay";

		simple-audio-card,cpu {
			sound-dai = <&rp1_audio_out>;
		};

		simple-audio-card,codec {
			sound-dai = <&spdif_out>;
		};
	};

	battery@0 {
		phandle = <0x10c>;
		charge-full-design-microamp-hours = <0x663be0>;
		energy-full-design-microwatt-hours = <0x17a43f0>;
		voltage-min-design-microvolt = <0x325aa0>;
		voltage-max-design-microvolt = <0x401640>;
		constant-charge-current-max-microamp = <0x200b20>;
		device-chemistry = "lithium-ion";
		compatible = "simple-battery";
		reg = <0x00 0x00>;
	};

	framebuffer {
		compatible = "simple-framebuffer";
		status = "disabled";
	};

	system {
		linux,serial = <0x5989360f 0x9840295>;
		linux,revision = <0xe04180>;
	};

	smbios {
